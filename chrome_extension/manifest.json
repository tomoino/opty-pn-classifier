{
  "name": "Opty: Optimistic Search Filter",
  "short_name": "Opty",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "I make your search results optimistic!",
  "icons": {
    "16": "./img/icon_16.png",
    "48": "./img/icon_48.png",
    "128": "./img/icon_128.png"
  },
  "content_scripts": [{
    "matches": ["https://www.google.com/search*"],
    "js": [
      "content.js"
    ]
  }]
}